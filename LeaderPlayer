package com.company.androidquizapp;


public class LeaderPlayer {

    String name, score;

    //leaderboard players constructor
    public LeaderPlayer(String n, String s)
    {
        name = n;
        score = s;
    }

    public String getName()
    {
        return name;
    }

    public int getScore()
    {
        return Integer.parseInt(score);
    }
}
